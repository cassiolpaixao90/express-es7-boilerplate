{
  "name": "express-es7-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Cássio Paixão <cassiopaixaoti@gmail.com",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3.8"
  },
  "scripts": {
    "start": "node dist/bin/www.js",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src -d dist -s",
    "dev": "nodemon src/bin/www.js",
    "test": "cross-env NODE_ENV=test jest",
    "cover": "npm run test -- --coverage",
    "lint": "eslint --fix src && prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "smid": "^0.1.1"
  },
  "dependencies": {
    "awilix-express": "^2.1.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.4"
  }
}
