    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.5",
    "express-validator": "^5.3.1",
    "http-status": "^1.3.2",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "palin": "^2.2.0",
