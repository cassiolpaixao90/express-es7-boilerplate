{
  "sourceMaps": "both",
  "plugins": ["transform-runtime", "transform-object-rest-spread", "transform-decorators-legacy"],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["source-map-support"],
      "ignore": ["**/__tests__/**"]
    }
  }
}
